{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn build:config && docusaurus start",
    "build": "yarn build:config && docusaurus build",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "swizzle": "docusaurus swizzle --typescript",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "format": "prettier . --write",
    "build:config": "swc config -d config && swc docusaurus.config.ts --config-file ./config/.swcrc -o docusaurus.config.js"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.18",
    "@docusaurus/plugin-pwa": "2.0.0-beta.18",
    "@docusaurus/preset-classic": "2.0.0-beta.18",
    "@docusaurus/theme-common": "2.0.0-beta.18",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "copy-text-to-clipboard": "3.0.1",
    "docusaurus-plugin-custom-tags": "workspace:^",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.18",
    "@docusaurus/types": "^2.0.0-beta.18",
    "@fec/remark-a11y-emoji": "3.1.0",
    "@swc/cli": "^0.1.56",
    "@swc/core": "^1.2.160",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/is-ci": "^3",
    "@types/node": "17.0.18",
    "@types/prettier": "2.4.4",
    "is-ci": "^3.0.1",
    "prettier": "2.6.1",
    "swc-loader": "^0.1.15",
    "typescript": "4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "workspaces": [
    "plugins/*"
  ],
  "packageManager": "yarn@3.2.0"
}
